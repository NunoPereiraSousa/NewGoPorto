    "sass": "^1.26.9",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vue-snotify": "^3.2.1",
    "vue-star-rating": "^1.6.1",
    "vue2-google-maps": "^0.10.7",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@types/google-maps": "^3.2.2",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-router": "^4.1.0",
    "@vue/cli-plugin-unit-jest": "^4.4.6",
